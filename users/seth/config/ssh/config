# vim:ft=gitconfig

# NOTE:
# In the event you're unable to push:
# ssh -T git@github.com
# then, likely:
# chmod 0700 ~/.ssh/config

# AddKeysToAgent yes
# UseKeychain yes

Host *
  # Always use SSH2.
  Protocol 2

  # Use a shared channel for all sessions to the same host,
  # instead of always opening a new one. This leads to much
  # quicker connection times.
  ControlMaster auto
  # # ControlPath ~/.ssh/control/%r@%h:%p
  ControlPath /tmp/ssh_mux_%r@%h:%p
  ControlPersist 1800

  #   # also this stuff
  #   Compression yes
  #   TCPKeepAlive yes
  #   ServerAliveInterval 20
  #   ServerAliveCountMax 10
  # IdentityFile ~/.ssh/seth-megabookpro
  IdentityAgent "~/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock"
  #   PubkeyAcceptedKeyTypes=+ssh-dss
  #   # UseKeychain yes #-> not supported in homebrew openssh (https://superuser.com/a/1172536)

  #   # https://superuser.com/a/1373701
  #   IPQoS lowdelay throughput
  #   # PubkeyAcceptedKeyTypes=+ssh-dss
  #   SendEnv TZ

# Host github.com gh
#   ControlMaster auto
#   ControlPersist 120
#   Hostname github.com
#   User git
#   IdentityFile ~/.ssh/seth-megabookpro
#   ForwardAgent yes
#
# Host megalithic.dev ocean
#   HostName megalithic.dev
#   User replicant
#   ControlPath /tmp/ssh_mux_%r@%h:%p
#   IdentityFile ~/.ssh/seth-megabookpro


# DevSpace Start launchdeck-portal-api.devspace
Host launchdeck-portal-api.devspace
  HostName localhost
  LogLevel error
  Port 10209
  IdentityFile "/Users/seth/.devspace/ssh/id_devspace_ecdsa"
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  User devspace
# DevSpace End launchdeck-portal-api.devspace
# DevSpace Start launchdeck-portal.devspace
Host launchdeck-portal.devspace
  HostName localhost
  LogLevel error
  Port 10860
  IdentityFile "/Users/seth/.devspace/ssh/id_devspace_ecdsa"
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  User devspace
# DevSpace End launchdeck-portal.devspace
